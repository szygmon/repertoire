<?xml version="1.0" encoding="utf-8"?>
<form addrulepath="/administrator/components/com_repertoire/models/rules">
    <fieldset>
		
        <field
            name="title"
            type="text"
            label="COM_REPERTOIRE_TITLE"
            description="COM_REPERTOIRE_TITLE_DESC"
            size="50"
            class="inputbox"
            default=""
        />
        <field
            name="artist"
            type="text"
            label="COM_REPERTOIRE_ARTIST"
            description="COM_REPERTOIRE_ARTIST_DESC"
            size="50"
            class="inputbox"
            default=""
        />
        <field
            name="category"
            type="category"
            extension="com_repertoire"
            label="COM_REPERTOIRE_CATEGORY"
            description="COM_REPERTOIRE_CATEGORY_DESC"
            class="inputbox"
            default="">
            <option value="0">JOPTION_SELECT_CATEGORY</option>
        </field>
        <field
            name="language"
            type="text"
            label="COM_REPERTOIRE_LANGUAGE"
            description="COM_REPERTOIRE_LANGUAGE_DESC"
            size="20"
            class="inputbox"
            default=""
        />
        <field
            name="youtube"
            type="text"
            label="COM_REPERTOIRE_YOUTUBE"
            description="COM_REPERTOIRE_CATEGORY_DESC"
            size="50"
            class="inputbox"
            default=""
        />
        <field
            name="demo"
            type="text"
            label="COM_REPERTOIRE_DEMO"
            description="COM_REPERTOIRE_CATEGORY_DESC"
            size="50"
            class="inputbox"
            default=""
        />
        <field
            name="id"
            type="hidden"
        />
    </fieldset>
</form>